{
    "name": "crocodicstudio/crudbooster",
    "description": "Webillium CMS Is CRUD Generator Based On Laravel Easy To Make An Advanced Backend",
    "type": "library",
    "license": "MIT",
    "keywords":[
        "laravel",
        "admin-panel",
        "backend",
        "crud-generator",
        "wevillium-cms",
        "crud",
        "generator"
    ],
    "authors": [
        {
            "name": "Milos",
            "email": "milos.cvijic@ollitweb.com"
        }
    ],
    "minimum-stability": "stable",
  "require": {
    "php": "^7.3|^8.0|^8.2", // Laravel 9 requires PHP 7.3 or higher
    "barryvdh/laravel-dompdf": "0.8.*",
    "doctrine/dbal": "^2.3|^3.0", // Updated to support Laravel 9 which may require doctrine/dbal 3.*
    "maatwebsite/excel": "^3.1", // Ensure this package is compatible with Laravel 9
    "intervention/imagecache":"^2.3", // Ensure this package is compatible with Laravel 9
    "laravel/framework": "^7.30|^8.0|^9.0", // Added support for Laravel 9
    "krlove/code-generator": "^1.0" // Ensure this package is compatible with Laravel 9
},

    "autoload": {
        "psr-4": {
          "crocodicstudio\\crudbooster\\": "src/",
          "Krlove\\EloquentModelGenerator\\": "src/"
        }
      },
    "autoload-dev": {
        "psr-4": {
            "Krlove\\Tests\\Integration\\": "tests/integration"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "crocodicstudio\\crudbooster\\CRUDBoosterServiceProvider"
            ]
        }
    }
}
